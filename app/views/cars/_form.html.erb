<div container="container d-flex justify-content-center">
  <%= simple_form_for @car do |f|  %>
    <%= f.input :brand, collection: Car::BRAND.sort %>
    <%= f.input :car_type %>
    <%= f.input :description %>
    <%= f.input :location %>
    <%= f.input :price %>
    <%= f.submit class: "btn btn-primary" %>
  <% end %>
</div>
